!
! Colors
#define bg          #262626
#define fg          #eeeeee
#define black       #565656
#define red         #B3314B
#define green       #008081
#define yellow      #ffc43d
#define blue        #22cccc
#define magenta     #ff55ff
#define cyan        #22cccc
#define white       #cccccc
#define black2      #5B5B5B
